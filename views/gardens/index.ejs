<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Gardens</title>
    <!-- bootstrap -->
    <link rel="stylesheet"
   href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <!-- my stylesheet -->
    <link rel="stylesheet" type="text/css" href="/css/style.css">
    <!-- typefaces -->
    <link href="https://fonts.googleapis.com/css?family=Ribeye+Marrow" rel="stylesheet">
  </head>
  <body>

    <header>
      <h1> Peruse the Gardens</h1>
      <nav>
        <a href="/">Home</a>
        <br />
        <a href="/gardens/new"> Plot a Garden </a>
        <br />
        <a href="/plants"> Plant Varieties </a>
      </nav>
    </header>

    <main>
      <ul>
        <% for (var i =0; i < gardens.length; i++) {%>
          <li>
            <a href="/gardens/<%=gardens[i]._id%>"><%=gardens[i].name%></a>
          </li>
          <!-- add vines to frame text? -->
        <%}%>
      </ul>
    </main>

  </body>
</html>
